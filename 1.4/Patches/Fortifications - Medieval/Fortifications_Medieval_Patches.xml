<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Fortifications - Medieval - Material</li>
		</mods>
		
		<match Class="PatchOperationSequence">
			<operations>
		
		
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defName="FT_TableMasonry"]/costList/Steel</xpath>
					<match Class="PatchOperationSetName">
						<xpath>/Defs/ThingDef[defName="FT_TableMasonry"]/costList/Steel</xpath>
						<name>DankPyon_IronIngot</name>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="FT_CastleWall" or defName="FT_CastleWallEmbrasures"]/designationCategory</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="FT_CastleWall" or defName="FT_CastleWallEmbrasures"]/designationCategory</xpath>
					</match>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName="FT_CastleWall" or defName="FT_CastleWallEmbrasures"]/researchPrerequisites</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/ThingDef[defName="FT_CastleWall" or defName="FT_CastleWallEmbrasures"]/researchPrerequisites</xpath>
					</match>
				</li>
		
			</operations>
		</match>
	</Operation>
	
</Patch>
