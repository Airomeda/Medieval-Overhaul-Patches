<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Bees</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				
				<!-- Tier 1 Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Hybrid_Honeycomb"]/butcherProducts/RB_WhiteHoney</xpath>
					<value>
						<RB_WhiteHoney>5</RB_WhiteHoney>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Amalgam_Honeycomb"]/butcherProducts/RB_BlueHoney</xpath>
					<value>
						<RB_BlueHoney>5</RB_BlueHoney>
					</value>
				</li>
				
				<!-- Tier 2 Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Alloy_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_BaseHoney>5</RB_BaseHoney>
							<RB_Steelwax>5</RB_Steelwax>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Neutro_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_PsychotropicHoney>5</RB_PsychotropicHoney>
							<RB_Brittlewax>5</RB_Brittlewax>
						</butcherProducts>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Adaptive_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_PsychotropicHoney>5</RB_PsychotropicHoney>
							<RB_Brittlewax>5</RB_Brittlewax>
						</butcherProducts>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Nutritious_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_Beeswax>5</RB_Beeswax>
							<RB_BaseHoney>2</RB_BaseHoney>
							<RB_NondescriptMeat>7</RB_NondescriptMeat>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Technological_Honeycomb"]/butcherProducts</xpath>
					<value>
						<butcherProducts>
							<RB_HeavyWax>20</RB_HeavyWax>
						</butcherProducts>
					</value>
				</li>
				
				<!-- Tier 3 -->
				<!-- Biome Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[
					defName="RB_Arctic_Honeycomb" or
					defName="RB_Woodland_Honeycomb" or
					defName="RB_Desert_Honeycomb" or
					defName="RB_Swamp_Honeycomb" or
					defName="RB_Desert_Honeycomb"
					]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_Beeswax>10</RB_Beeswax>
							<RB_AdaptiveHoney>5</RB_AdaptiveHoney>
						</butcherProducts>
					</value>
				</li>
				
				<!-- Drug Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[
					defName="RB_Tipsy_Honeycomb" or
					defName="RB_Luxurious_Honeycomb" or
					defName="RB_Psychic_Honeycomb" or
					defName="RB_Stoner_Honeycomb"
					]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_Brittlewax>2</RB_Brittlewax>
							<RB_PsychotropicHoney>8</RB_PsychotropicHoney>
						</butcherProducts>
					</value>
				</li>
				
				<!-- Miner Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[
					defName="RB_Argent_Honeycomb" or
					defName="RB_Aurum_Honeycomb" or
					defName="RB_Precious_Honeycomb" or
					defName="RB_Radioactive_Honeycomb"
					]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_Steelwax>10</RB_Steelwax>
							<RB_BaseHoney>5</RB_BaseHoney>
						</butcherProducts>
					</value>
				</li>
				
				<!-- Agriculture Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[
					defName="RB_Agricultural_Honeycomb" or
					defName="RB_Bittersweet_Honeycomb" or
					defName="RB_Lactic_Honeycomb" or
					defName="RB_Egglaying_Honeycomb" or
					defName="RB_Insectoid_Honeycomb"
					]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_Beeswax>5</RB_Beeswax>
							<RB_TastyHoney>2</RB_TastyHoney>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Agricultural_Honeycomb"]/butcherProducts</xpath>
					<value>
						<RB_NonDescriptVegetables>5</RB_NonDescriptVegetables>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Bittersweet_Honeycomb"]/butcherProducts</xpath>
					<value>
						<Chocolate>5</Chocolate>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Lactic_Honeycomb"]/butcherProducts</xpath>
					<value>
						<RB_BeeMilk>5</RB_BeeMilk>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Egglaying_Honeycomb"]/butcherProducts</xpath>
					<value>
						<RB_BeeEggs>5</RB_BeeEggs>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="RB_Insectoid_Honeycomb"]/butcherProducts</xpath>
					<value>
						<InsectJelly>5</InsectJelly>
					</value>
				</li>
				
				<!-- Tech Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[
					defName="RB_Electronic_Honeycomb" or
					defName="RB_Oily_Honeycomb" or
					defName="RB_Plastic_Honeycomb"
					]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_HeavyWax>10</RB_HeavyWax>
							<RB_BaseHoney>5</RB_BaseHoney>
						</butcherProducts>
					</value>
				</li>
				
				<!-- Tech Bees -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[
					defName="RB_Pet_Honeycomb" or
					defName="RB_Straw_Honeycomb" or
					defName="RB_Weaving_Honeycomb" or
					defName="RB_Tanner_Honeycomb"
					]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_TastyHoney>15</RB_TastyHoney>
							<RB_HeavyWax>5</RB_HeavyWax>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Synthetic_Honeycomb"]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_BaseHoney>5</RB_BaseHoney>
							<RB_Beeswax>25</RB_Beeswax>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Hyper_Honeycomb"]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_TastyHoney>5</RB_TastyHoney>
							<RB_Beeswax>40</RB_Beeswax>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Surgeon_Honeycomb"]/butcherProducts/MedicineUltratech
					</xpath>
					<value>
						<MedicineHerbal>5</MedicineHerbal>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Lucifer_Honeycomb"]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<Luciferium>1</Luciferium>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Nitro_Honeycomb"]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_HeavyWax>10</RB_HeavyWax>
							<RB_WhiteHoney>15</RB_WhiteHoney>
						</butcherProducts>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="RB_Nitro_Honeycomb"]/butcherProducts
					</xpath>
					<value>
						<butcherProducts>
							<RB_HeavyWax>15</RB_HeavyWax>
							<RB_WhiteHoney>15</RB_WhiteHoney>
						</butcherProducts>
					</value>
				</li>
				
				
			</operations>
		</match>
	</Operation>
	
</Patch>