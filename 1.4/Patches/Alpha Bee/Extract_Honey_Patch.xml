<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Bees</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="DankPyon_Extract_Honey" or defName="DankPyon_Extract_HoneyBulk"]/recipeUsers</xpath>
					<value>
						<recipeUsers>
							<li>RB_Beekeeping_Workbench_Building</li>
						</recipeUsers>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="DankPyon_Extract_Honey"]/label</xpath>
					<value>
						<label>extract from carrion comb</label>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName="DankPyon_Extract_HoneyBulk"]/label</xpath>
					<value>
						<label>extract from carrion comb x5</label>
					</value>
				</li>
				
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="RB_CentrifugeAnyComb"]/recipeUsers/li[.="RB_Beekeeping_Workbench_Building"]</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/RecipeDef[defName="RB_CentrifugeAnyComb"]</xpath>
						<value>
							<recipeUsers>
								<li>RB_Beekeeping_Workbench_Building</li>
							</recipeUsers>
						</value>
					</nomatch>
				</li>
				
			</operations>
		</match>
	</Operation>
	
</Patch>