<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ReGrowth: Core</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="RG_MushroomCultivation" or defName="RG_BerryCultivation"]/tab</xpath>
					<value>
						<tab>
							<li>DankPyon_MedievalResearchTab</li>
						</tab>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="RG_MushroomCultivation"]/researchViewX</xpath>
					<value>
						<researchViewX>0.00</researchViewX>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="RG_MushroomCultivation"]/researchViewY</xpath>
					<value>
						<researchViewY>3.40</researchViewY>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="RG_BerryCultivation"]/researchViewX</xpath>
					<value>
						<researchViewX>1.00</researchViewX>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="RG_BerryCultivation"]/researchViewY</xpath>
					<value>
						<researchViewY>0.60</researchViewY>
					</value>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ResearchTabDef[defName="RG_ReGrowth"]</xpath>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>