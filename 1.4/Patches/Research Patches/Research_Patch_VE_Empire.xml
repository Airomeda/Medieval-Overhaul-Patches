<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Empire</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_PipeOrgans" or defName="VFEE_NobleDecorations" or defName="VFEE_StellicThrone"]</xpath>
					<value>
						<tab>DankPyon_MedievalResearchTab</tab>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_NobleDecorations"]/researchViewX</xpath>
					<value>
						<researchViewX>5.00</researchViewX>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_NobleDecorations"]/researchViewY</xpath>
					<value>
						<researchViewY>4.10</researchViewY>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_StellicThrone"]/researchViewX</xpath>
					<value>
						<researchViewX>6.00</researchViewX>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_StellicThrone"]/researchViewY</xpath>
					<value>
						<researchViewY>4.10</researchViewY>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_PipeOrgans"]/researchViewX</xpath>
					<value>
						<researchViewX>6.00</researchViewX>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="VFEE_PipeOrgans"]/researchViewY</xpath>
					<value>
						<researchViewY>4.70</researchViewY>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
	
</Patch>
