<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul Deep Vein Mining</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="MODeepMining"]/researchViewX</xpath>
					<value>
						<researchViewX>16</researchViewX>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="MODeepMining"]/researchViewY</xpath>
					<value>
						<researchViewY>4.05</researchViewY>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>