<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[LTS]Furnishing</li>
		</mods>
			
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="LTS_Firepit" or defName="LTS_Fireplace" or defName="LTS_FirepitRound"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs</xpath>
						<value>
							<li>DankPyon_RawWood</li>
							<li>DankPyon_RawDarkWood</li>
							<li>DankPyon_DarkWoodLog</li>
							<li>DankPyon_ElvenWoodLog</li>
							<li>DankPyon_DryadWoodLog</li>
							<li>DankPyon_AncientWoodLog</li>
						</value>
					</li>
					
			</operations>
		</match>
	</Operation>
  
</Patch>