<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Fortifications - Neolithic</li>
		</mods>
	
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[@Name="FT_Palisade_Base"]/stuffCategories</xpath>
					<value>
						<stuffCategories>
							<li>DankPyon_RawWood</li>
						</stuffCategories>
					</value>
				</li>
				
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Palisade" or defName="DankPyon_EmbPalisade"]/designationCategory</xpath>
				</li>
				
			</operations>
		</match>
			
	</Operation>
		
</Patch>
