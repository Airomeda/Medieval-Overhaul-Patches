<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
	
				<!-- Cheese Comp Properties -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="DankPyon_MealCheese" or defName="DankPyon_MealGoatCheese" or defName="DankPyon_MealSheepCheese"]/comps</xpath>
					<value>
						<li>
							<compClass>CompQuality</compClass>
						</li>
						<li Class="VanillaCookingExpanded.CompProperties_StackByQuality"/>
						<li Class="VFECore.CompProperties_IngestedThoughtFromQuality">
							<ingestedThought>VCE_AteCheese</ingestedThought>
						</li>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="DankPyon_Wine"]/comps</xpath>
					<value>
						<li Class="VanillaCookingExpanded.CompProperties_StackByQuality"/>
					</value>
				</li>
				
			</operations>
		</match>
		
	</Operation>
	
</Patch>