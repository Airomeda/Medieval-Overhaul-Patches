<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Campfire" or defName="TorchLamp"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs</xpath>
		<value>
			<li>DankPyon_RawWood</li>
			<li>DankPyon_RawDarkWood</li>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Ideology</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="Darktorch"]/comps/li[@Class="CompProperties_Refuelable"]/fuelFilter/thingDefs</xpath>
			<value>
				<li>DankPyon_RawWood</li>
				<li>DankPyon_RawDarkWood</li>
			</value>
		</match>
	</Operation>
	
</Patch>