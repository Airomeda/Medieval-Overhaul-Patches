<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded - More Plants</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

			<!-- Fruit Tree Integration -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[
					defName="VCE_TreeAvocado" or
					defName="VCE_TreeOlive" or
					defName="VCE_CoconutPalm" or
					defName="VCE_TreeAlmond" or
					defName="VCE_TreeMango"]/plant
					</xpath>
					<value>
						<sowResearchPrerequisites>
						<li>Cocoa</li>
						</sowResearchPrerequisites>
					</value>
				</li>
			

			</operations>
		</match>
	</Operation>
	
</Patch>