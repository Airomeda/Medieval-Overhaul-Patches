						</value>
					</match>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeBlueberryMilkshakeSmall" or defName="Sierra_MakeBlueberryMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
							<thingDefs>
								<li>Sierra_Blueberries</li>
							</thingDefs>
						</fixedIngredientFilter>
					</value>
				</li>
				
				<!-- Chocolate Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeChocolateMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
