						<value>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeBerryMilkshakeSmall" or defName="Sierra_MakeBerryMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
							<thingDefs>
								<li>RawBerries</li>
							</thingDefs>
						</fixedIngredientFilter>
					</value>
				</li>
				
				<!-- Blueberry Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeBlueberryMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
