							</categories>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeChocolateMilkshakeSmall" or defName="Sierra_MakeChocolateMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
							<thingDefs>
								<li>Chocolate</li>
							</thingDefs>
						</fixedIngredientFilter>
					</value>
				</li>
				
				<!-- Mixed Berry Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeMixedBerryMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="Sierra_MakeMixedBerryMilkshakeSmall" or defName="Sierra_MakeMixedBerryMilkshakeMedium"]/ingredients/li[1]/filter/thingDefs</xpath>
						<value>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeMixedBerryMilkshakeSmall" or defName="Sierra_MakeMixedBerryMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
							<thingDefs>
								<li>RawBerries</li>
							</thingDefs>
						</fixedIngredientFilter>
					</value>
				</li>
				
				<!-- Mixed Berry Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeStrawberryMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="Sierra_MakeStrawberryMilkshakeSmall" or defName="Sierra_MakeStrawberryMilkshakeMedium"]/ingredients/li[1]/filter/thingDefs</xpath>
						<value>
							<categories>
								<li>DankPyon_Milk</li>
