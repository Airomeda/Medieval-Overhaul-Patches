				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeStrawberryMilkshakeSmall" or defName="Sierra_MakeStrawberryMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
							<thingDefs>
								<li>Sierra_Strawberries</li>
							</thingDefs>
						</fixedIngredientFilter>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>