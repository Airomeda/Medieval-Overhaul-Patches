<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Milkshakes - Digital Deluxe Edition</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
				<!-- Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="Sierra_MakeMilkshakeSmall" or defName="Sierra_MakeMilkshakeMedium"]/ingredients/li[1]/filter/thingDefs</xpath>
						<value>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeMilkshakeSmall" or defName="Sierra_MakeMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
						</fixedIngredientFilter>
					</value>
				</li>

				<!-- Ambrosia Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeAmbrosiaMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
					<match Class="PatchOperationReplace">
						<xpath>Defs/RecipeDef[defName="Sierra_MakeAmbrosiaMilkshakeSmall" or defName="Sierra_MakeAmbrosiaMilkshakeMedium"]/ingredients/li[1]/filter/thingDefs</xpath>
						<value>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
						</value>
					</match>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeAmbrosiaMilkshakeSmall" or defName="Sierra_MakeAmbrosiaMilkshakeMedium"]</xpath>
					<value>
						<fixedIngredientFilter>
							<categories>
								<li>DankPyon_Milk</li>
							</categories>
							<thingDefs>
								<li>Ambrosia</li>
								<li>MedicineHerbal</li>
							</thingDefs>
						</fixedIngredientFilter>
					</value>
				</li>
				
				<!-- Berry Milkshakes -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/RecipeDef[defName="Sierra_MakeBerryMilkshakeSmall"]/ingredients/li[1]/filter/thingDefs</xpath>
