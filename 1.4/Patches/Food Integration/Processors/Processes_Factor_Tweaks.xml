<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Process Sun Factor -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_DriedMeatProcess" or defName="DankPyon_StrawProcess"]/sunFactor</xpath>
		<value>
			<sunFactor>0.5~1.0</sunFactor>
		</value>
	</Operation>
	
	<!-- Process Wind Factor -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_DriedMeatProcess" or defName="DankPyon_StrawProcess"]/windFactor</xpath>
		<value>
			<windFactor>0.75~1.25</windFactor>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_MustIntoWine"]/efficiency</xpath>
		<value>
			<efficiency>1.0</efficiency>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_MustIntoWine"]/usesQuality</xpath>
		<value>
			<usesQuality>true</usesQuality>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_MustIntoWine"]/qualityDays</xpath>
		<value>
			<qualityDays>(1, 3, 8, 14, 20, 50, 120)</qualityDays>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_HoneyIntoMead"]/usesQuality</xpath>
		<value>
			<usesQuality>true</usesQuality>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_HoneyIntoMead"]/qualityDays</xpath>
		<value>
			<qualityDays>(1, 3, 8, 14, 20, 50, 120)</qualityDays>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_PressCheeseProcess"]/usesQuality</xpath>
		<value>
			<usesQuality>true</usesQuality>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_PressCheeseProcess"]/qualityDays</xpath>
		<value>
			<qualityDays>(1, 3, 8, 14, 20, 50, 120)</qualityDays>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_PressGoatCheeseProcess"]/usesQuality</xpath>
		<value>
			<usesQuality>true</usesQuality>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ProcessorFramework.ProcessDef[defName="DankPyon_PressGoatCheeseProcess"]/qualityDays</xpath>
		<value>
			<qualityDays>(1, 3, 8, 14, 20, 50, 120)</qualityDays>
		</value>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>XML Extensions</li>
		</mods>
		<nomatch Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="DankPyon_MealCheese" or defName="DankPyon_MealGoatCheese" or defName="DankPyon_MealSheepCheese"]/comps</xpath>
					<value>
						<li>
							<compClass>CompQuality</compClass>

						</li>
						<li Class="VFECore.CompProperties_IngestedThoughtFromQuality">
							<ingestedThought>DankPyon_AteCheese</ingestedThought>
						</li>
					</value>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_MealCheese" or defName="DankPyon_MealGoatCheese" or defName="DankPyon_MealSheepCheese"]/ingestible/tasteThought</xpath>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="DankPyon_Wine" or defName="DankPyon_Mead"]/comps</xpath>
					<value>
						<li>
							<compClass>CompQuality</compClass>
						</li>
						<li Class="VFECore.CompProperties_IngestedThoughtFromQuality">
							<ingestedThought>DankPyon_DrankWine</ingestedThought>
						</li>
					</value>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[defName="DankPyon_Wine" or defName="DankPyon_Mead"]/ingestible/tasteThought</xpath>
				</li>

			</operations>
		</nomatch>
	</Operation>

</Patch>