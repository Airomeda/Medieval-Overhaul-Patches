<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Prosthetics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				
				<!-- Basic Prosthetics -->
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[
					defName="WoodenFoot" or
					defName="WoodenToe" or
					defName="WoodenFinger" or
					defName="EyePatch" or
					defName="EarPatch" or
					defName="IronArm" or
					defName="ProstheticHook"]/costList
					</xpath>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="IronArm" or defName="ProstheticHook"]
					</xpath>
					<value>
						<stuffCategories>
							<li>Metallic</li>
						</stuffCategories>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[
					defName="WoodenFoot" or
					defName="WoodenToe" or
					defName="WoodenFinger"]
					</xpath>
					<value>
						<stuffCategories>
							<li>Woody</li>
						</stuffCategories>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[
					defName="EyePatch" or
					defName="EarPatch"]
					</xpath>
					<value>
						<stuffCategories>
							<li>Fabric</li>
						</stuffCategories>
					</value>
				</li>
			
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="IronArm"]</xpath>
					<value>
						<costStuffCount>50</costStuffCount>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="ProstheticHook"]</xpath>
					<value>
						<costStuffCount>25</costStuffCount>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="WoodenFoot"]</xpath>
					<value>
						<costStuffCount>25</costStuffCount>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="WoodenToe"]</xpath>
					<value>
						<costStuffCount>50</costStuffCount>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="WoodenFinger"]</xpath>
					<value>
						<costStuffCount>5</costStuffCount>
