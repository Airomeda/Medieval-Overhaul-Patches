<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Prosthetics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
		
				<!-- Basic Prosthetics -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="Basic"]/recipeMaker/recipeUsers
					</xpath>
					<value>
						<li>DankPyon_Workbench</li>
					</value>
				</li>
				

				<!-- ArtificialMed -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="ArtificialMed"]/recipeMaker/recipeUsers
					</xpath>
					<value>
						<li>DankPyon_Workbench</li>
					</value>
				</li>

			
				<!-- MechanicalMed -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="MechanicalMed"]/recipeMaker/recipeUsers
					</xpath>
					<value>
						<li>DankPyon_Workbench</li>
					</value>
				</li>
				
				<!-- AmbroisenMed -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="AmbroisenMed"]/recipeMaker/recipeUsers
					</xpath>
					<value>
						<li>DankPyon_Workbench</li>
					</value>
				</li>

			</operations>
		</match>
		
	</Operation>

</Patch>
