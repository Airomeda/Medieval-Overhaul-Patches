<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Ideology</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Dryad_Woodmaker"]/comps/li[@Class="CompProperties_Spawner"]/thingToSpawn</xpath>
					<value>
						<thingToSpawn>DankPyon_RawWood</thingToSpawn>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="Dryad_Woodmaker"]/descriptionHyperlinks</xpath>
					<value>
						<descriptionHyperlinks>
							<ThingDef>DankPyon_RawWood</ThingDef>
						</descriptionHyperlinks>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>