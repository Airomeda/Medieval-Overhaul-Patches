<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>pphhyy Sanguinary Animals</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="pphhyy_SanguineDeer"]/race/wildBiomes</xpath>
					<value>
						<DankPyon_AncientForest>0.6</DankPyon_AncientForest>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="pphhyy_Vargulf"]/race/wildBiomes</xpath>
					<value>
						<DankPyon_AncientForest>0.08</DankPyon_AncientForest>
					</value>
				</li>

				<li Class="PatchOperationFindMod">
					<mods>
						<li>Biotech</li>
					</mods>
					<match Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="pphhyy_BloodScarab"]/race/wildBiomes</xpath>
						<value>
							<DankPyon_AncientForest>1.2</DankPyon_AncientForest>
						</value>
					</match>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>
