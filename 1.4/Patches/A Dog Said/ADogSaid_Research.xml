<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>A Dog Said... Animal Prosthetics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="SimpleAnimalProsthetics"]/tab</xpath>
					<value>
						<tab>
							<li>DankPyon_MedievalResearchTab</li>
						</tab>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="SimpleAnimalProsthetics"]/researchViewX</xpath>
					<value>
						<researchViewX>8.00</researchViewX>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ResearchProjectDef[defName="SimpleAnimalProsthetics"]/researchViewY</xpath>
					<value>
						<researchViewY>4.20</researchViewY>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>
