<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Optimization: Meats - C# Edition</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<!-- Excludes the races below from the meat optimization -->
					<xpath>/Defs/AlienMeatTest.MeatListDef[defName="WhiteList"]/races</xpath>
					<value>
						<li>DankPyon_Rox</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>